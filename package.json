{
  "name": "@luvabase/sdk",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate-manifest-json-schema": "bun -e \"import * as t from './tools/tools'; t.generateManifestJsonSchema()\"",
